[run]
source =
    engine
    services
    app
    veg_change_engine
branch = True
omit =
    */tests/*
    */__pycache__/*
    */migrations/*
    */.venv/*
    */venv/*
    setup.py
    conftest.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    @abc.abstractmethod
show_missing = True
precision = 2

[html]
directory = htmlcov
title = Verdant Test Coverage

[xml]
output = coverage.xml
